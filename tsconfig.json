{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "baseUrl": ".",
    "allowJs": true,
    "checkJs": true,
    "strict": true
  },
  "include": [
    "dataloader/download.ts",
    "dataloader/server.js",
    "src/**/*.d.ts",
    "src/**/*.ts",
    "src/**/*.js",
    "src/**/*.svelte"
  ]
}
